    "from itertools import product\n",
    "\n",
    "options1 = ['A', 'C', 'G']\n",
    "options2 = ['T', 'C', 'C']\n",
    "options3 = ['T', 'C', 'C']\n",
    "options4 = ['T', 'C', 'C']\n",
    "all = []\n",
    "for x in map(''.join, product(*zip(options1, options2, options3, options4))):\n",
    "    all.append(x)\n",
    "print(len(all))"
   ]
